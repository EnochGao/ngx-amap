{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-amap": [
        "dist/ngx-amap"
      ],
      "ngx-amap/types/class": [
        "dist/ngx-amap/types/class"
      ],
      "ngx-amap/types/interface": [
        "dist/ngx-amap/types/interface"
      ]
    }
  }
}